{
    "name": "mohib/laravel-api-repository",
    "description": "A Laravel package for implementing the Repository Pattern in REST APIs.",
    "authors": [
        {
            "name": "Md. Mohib Ullah",
            "email": "mohib@example.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "illuminate/support": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Mohib\\LaravelApiRepository\\": "src/",
            "Mohib\\LaravelApiRepository\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mohib\\LaravelApiRepository\\LaravelApiRepositoryServiceProvider"
            ],
            "aliases": {
                "ApiRepository": "Mohib\\LaravelApiRepository\\Facades\\ApiRepository"
            }
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "orchestra/testbench": "9.x-dev"
    }
}
